{
    "name": "simple-zendesk-garden-react-app",
    "version": "1.1.0",
    "description": "A simple Zendesk app template using React and Zendesk Garden",
    "main": "dist/assets/index.html",
    "author": "James Ratcliffe",
    "license": "MIT",
    "private": true,
    "scripts": {
        "watch": "parcel watch src/index.js --out-dir dist/assets",
        "build": "parcel build src/index.js --out-dir dist/assets",
        "serve": "zcli apps:server dist",
        "bump": "bash scripts/bump_all.sh",
        "package": "yarn build && zcli apps:package dist && zcli apps:clean dist",
        "update": "yarn build && zcli apps:update"
    },
    "dependencies": {
        "@svgr/parcel-plugin-svgr": "^5.5.0",
        "@zendeskgarden/css-bedrock": "^8.0.1",
        "@zendeskgarden/react-buttons": "^8.28.0",
        "@zendeskgarden/react-grid": "^8.28.0",
        "@zendeskgarden/react-theming": "^8.1.0",
        "@zendeskgarden/react-typography": "^8.28.0",
        "@zendeskgarden/svg-icons": "^6.26.0",
        "prop-types": "^15.6.1",
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "react-is": "^16.8.0",
        "styled-components": "^4.2.0 || ^5.0.0"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4"
    },
    "browserslist": [
        "last 1 Chrome versions",
        "last 1 Firefox versions"
    ]
}
